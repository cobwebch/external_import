			<trans-unit id="no.error" xml:space="preserve">
				<source>No errors</source>
			</trans-unit>
			<trans-unit id="no.success" xml:space="preserve">
				<source>No success messages</source>
			</trans-unit>
			<trans-unit id="allTables" xml:space="preserve">
				<source>All tables</source>
			</trans-unit>
			<trans-unit id="tableAndIndex" xml:space="preserve">
				<source>Table: %1$s, index: %2$s</source>
			</trans-unit>
			<trans-unit id="exceptionOccurred" xml:space="preserve">
				<source>A problem occurred. The following error was returned: %1$s (Exception code: %2$d).</source>
			</trans-unit>
			<trans-unit id="invalidCustomHandler" xml:space="preserve">
				<source>Class %s does not implement the \Cobweb\ExternalImport\DataHandlerInterface interface.</source>
			</trans-unit>
			<trans-unit id="wrongCustomHandler" xml:space="preserve">
				<source>Could not instantiate class %s as a custom data handler.</source>
			</trans-unit>
			<trans-unit id="noConfigurationFound" xml:space="preserve">
